<%- include('../partials/header')%>

<table id="list">
    <h1>Nutrition</h1>
    <thead>
    <tr>
        <th>Name</th>
        <th>Time<br>Cooking</th>
    </tr>
    </thead>  
    <tbody>
    <% recipes.forEach(function(r) { %>
        <tr>
        <td><%= r.name %></td>
        <td><%= r.timeCooking %></td>
        <td><a href="/recipes/<%= r._id%>">Details</a></td>
      </tr>
   <% }); %>
</tbody>
</table>

<%- include('../partials/footer')%>