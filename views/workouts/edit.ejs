<%- include('../partials/header')%>

<section id="edit-form">
    <form action="/workouts/<%=workout._id %>?_method=PUT" method="POST">
     
     <label>Name of Workout:
     <input type="text" name="title" 
     value="<%= workout.title%>">
    </label><br>
     <label>Level:
     <select name="level" value="<%= workout.level%>">
        <option value="Beginner">Beginner(easy)</option>
        <option value="Intermediate">Intermediate(medium)</option>
        <option value="Experienced">Experienced(hard)</option>
    </select>
    </label><br>
    <label>Completed?:
        <input type="text" name="Completed" value="<%= workout.completed%>">
       
    </label><br>
     <button type="submit">Save</button>
    </form>






<%- include('../partials/footer')%>